#
# Print help for findLockByEnvironmentName operation
#
##############################################################################
print_findLockByEnvironmentName_help() {
    echo ""
    echo -e "$(tput bold)$(tput setaf 7)findLockByEnvironmentName - Find lock by environment ame$(tput sgr0)"
    echo -e ""
    echo -e "Returns a single lock which is owned by the specified environment, or zero if not found" | fold -sw 80
    echo -e ""
    echo -e "$(tput bold)$(tput setaf 7)Parameters$(tput sgr0)"
    echo -e "  * $(tput setaf 2)envName$(tput sgr0) $(tput setaf 4)[String]$(tput sgr0) $(tput setaf 1)(required)$(tput sgr0)$(tput sgr0) - Name of environment that owns lock$(tput setaf 3) Specify as: envName=value$(tput sgr0)" \
        | fold -sw 80 | sed '2,$s/^/    /'
    echo ""
    echo -e "$(tput bold)$(tput setaf 7)Responses$(tput sgr0)"
    case 200 in
        1*)
        echo -e "$(tput setaf 7)  200;successful operation$(tput sgr0)" | column -t -s ';' | fold -sw 80 | sed '2,$s/^/       /'
